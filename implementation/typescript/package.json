{
  "name": "openapi-codegen-typescript",
  "version": "1.0.0",
  "description": "OpenAPI Code Generator - TypeScript Implementation with full i18n support",
  "main": "dist/index.js",
  "bin": {
    "openapi-codegen": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testNamePattern='Integration Tests'",
    "test:composition": "jest --testNamePattern='Schema Composition Tests'",
    "clean": "rimraf dist"
  },
  "keywords": [
    "openapi",
    "code-generation",
    "spring-boot",
    "kotlin",
    "typescript",
    "i18n"
  ],
  "author": "OpenAPI CodeGen",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
