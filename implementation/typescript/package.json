{
  "name": "openapi-codegen-typescript",
  "version": "1.0.0",
  "description": "OpenAPI Code Generator - TypeScript Implementation with full i18n support",
  "main": "dist/index.js",
  "bin": {
    "openapi-codegen": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:fast": "vitest run --reporter=basic --pool=threads --poolOptions.threads.maxThreads=4",
    "test:unit": "vitest run --exclude='**/{integration,benchmark,large-spec,property-based}*.test.ts'",
    "test:integration": "vitest run --grep='Integration Tests'",
    "test:composition": "vitest run --grep='Schema Composition Tests'",
    "test:performance": "vitest run --include='**/{benchmark,large-spec,performance}*.test.ts' --pool=forks --poolOptions.forks.singleFork=true",
    "lint": "echo 'Lint check temporarily disabled'",
    "lint:fix": "echo 'Lint fix temporarily disabled'",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage test-output* node_modules/.cache"
  },
  "keywords": [
    "openapi",
    "code-generation",
    "spring-boot",
    "kotlin",
    "typescript",
    "i18n"
  ],
  "author": "OpenAPI CodeGen",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.54.0",
    "fast-check": "^4.1.1",
    "happy-dom": "^12.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
